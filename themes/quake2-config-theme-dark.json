{
  "$schema": "vscode://schemas/color-theme",
  "name": "Quake2ConfigThemeDark",
  "tokenColors": [
    {
      "name": "commands-default",
      "scope": [
        "support.function.q2config"
      ],
      "settings": {
        "foreground": "#7ab6f7"
      }
    },
    {
      "name": "commands-default-actions",
      "scope": [
        "support.function.q2config.commands.actions"
      ],
      "settings": {
        "fontStyle": "italic",
      }
    },
    {
      "name": "commands-default-functions",
      "scope": [
        "support.function.q2config.commands.functions"
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "name": "commands-default-operations",
      "scope": [
        "support.function.q2config.commands.operations"
      ],
      "settings": {
        "fontStyle": "underline",
      }
    },
    {
      "name": "commands-q2pro-functions",
      "scope": [
        "support.function.q2config.commands.functions.q2pro"
      ],
      "settings": {
        "foreground": "#048BA8"
      }
    },
    {
      "name": "commands-q2pro-operations",
      "scope": [
        "support.function.q2config.commands.operations.q2pro"
      ],
      "settings": {
        "foreground": "#048BA8"
      }
    },
    {
      "name": "commands-openffa-operations",
      "scope": [
        "support.function.q2config.commands.operations.openffa"
      ],
      "settings": {
        "foreground": "#94d1d2"
      }
    },
    {
      "name": "commands-openffa-functions",
      "scope": [
        "support.function.q2config.commands.functions.openffa"
      ],
      "settings": {
        "foreground": "#94d1d2"
      }
    },
    {
      "name": "commands-opentdm-all",
      "scope": [
        "support.function.q2config.commands.unsorted.opentdm"
      ],
      "settings": {
        "foreground": "#61d0e7"
      }
    },
    {
      "name": "commands-ctf-all",
      "scope": [
        "support.function.q2config.commands.unsorted.ctf"
      ],
      "settings": {
        "foreground": "#9cb7bc"
      }
    },
    {
      "name": "commands-3zb2-all",
      "scope": [
        "support.function.q2config.commands.unsorted.3zb2"
      ],
      "settings": {
        "foreground": "#537378"
      }
    },
    {
      "name": "commands-yamagi-all",
      "scope": [
        "support.function.q2config.commands.unsorted.yamagi-quake2"
      ],
      "settings": {
        "foreground": "#11cccf"
      }
    },
    {
      "name": "commands-q2enhanced-all",
      "scope": [
        "support.function.q2config.commands.unsorted.q2enhanced",
        "support.function.q2config.commands.actions.q2enhanced"
      ],
      "settings": {
        "foreground": "#ff8001",
        "fontStyle": "bold"
      }
    },
    {
      "name": "cvars-default",
      "scope": [
        "variable.q2config.cvars"
      ],
      "settings": {
        "foreground": "#ca6702"
      }
    },
    {
      "name": "cvars-q2pro-engine",
      "scope": [
        "variable.q2config.cvars.engine.q2pro"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ca6702"
      }
    },
    {
      "name": "cvars-q2pro-game",
      "scope": [
        "variable.q2config.cvars.game.q2pro"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ca6702"
      }
    },
    {
      "name": "cvars-vanilla",
      "scope": [
        "variable.q2config.cvars.engine.vanilla"
      ],
      "settings": {
        "foreground": "#bb3e03"
      }
    },
    {
      "name": "cvars-openffa",
      "scope": [
        "variable.q2config.cvars.game.openffa"
      ],
      "settings": {
        "foreground": "#bb9c03"
      }
    },
    {
      "name": "cvars-opentdm",
      "scope": [
        "variable.q2config.cvars.game.opentdm"
      ],
      "settings": {
        "foreground": "#b5bb03"
      }
    },
    {
      "name": "cvars-yamagi-ctf",
      "scope": [
        "variable.q2config.cvars.game.yamagi-ctf"
      ],
      "settings": {
        "foreground": "#b6a150"
      }
    },
    {
      "name": "cvars-yamagi-3zb2",
      "scope": [
        "variable.q2config.cvars.game.yamagi-3zb2"
      ],
      "settings": {
        "foreground": "#aea47e"
      }
    },
    {
      "name": "cvars-q2enhanced",
      "scope": [
        "variable.q2config.cvars.unsorted.q2enhanced"
      ],
      "settings": {
        "foreground": "#fab46e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "keys-default",
      "scope": [
        "keyword.control.q2config"
      ],
      "settings": {
        "foreground": "#ff30a5"
      }
    },
    {
      "name": "keys-q2pro",
      "scope": [
        "keyword.control.q2config.specialkeys.q2pro"
      ],
      "settings": {
        "foreground": "#c95d93"
      }
    },
    {
      "name": "useable-items",
      "scope": [
        "constant.character.q2config.useableItems"
      ],
      "settings": {
        "foreground": "#D49A6A"
      }
    },
    {
      "name": "q2proMacros",
      "scope": [
        "support.class.q2config.macros.q2pro"
      ],
      "settings": {
        "foreground": "#ffc89e"
      }
    },
    {
      "name": "strings",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#328625"
      }
    },
    {
      "name": "numbers",
      "scope": [
        "constant.numeric.q2config"
      ],
      "settings": {
        "foreground": "#eec600"
      }
    },
    {
      "name": "specSymbols",
      "scope": [
        "constant.character.q2config"
      ],
      "settings": {
        "foreground": "#ae2012"
      }
    },
    {
      "name": "comments",
      "scope": [
        "comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#666666"
      }
    }
  ]
}
